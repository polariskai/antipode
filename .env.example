# Environment variables for Antipode Adversarial AML System

# Neo4j Database Configuration (optional - for graph features)
NEO4J_PASSWORD=your_neo4j_password
NEO4J_DATABASE=antipode_synth_data

# LLM Provider (set ONE of these)
GROQ_API_KEY=your_groq_api_key_here
# OPENAI_API_KEY=your_openai_api_key_here

# Databricks MLflow Configuration (optional - for experiment tracking)
DATABRICKS_HOST=https://your-databricks-instance.cloud.databricks.com
DATABRICKS_TOKEN=your_databricks_token_here
MLFLOW_TRACKING_URI=databricks
MLFLOW_EXPERIMENT_ID=your_experiment_id

# Enable MLflow tracing
MLFLOW_ENABLE_SYSTEM_METRICS_LOGGING=true

# PostgreSQL Configuration (AWS RDS)
BANK_POSTGRES_PASSWORD=your_postgres_password
POSTGRES_HOST=your-rds-instance.region.rds.amazonaws.com
